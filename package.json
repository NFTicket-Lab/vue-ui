{
  "name": "docs",
  "version": "1.0.0",
  "description": "docs for vue-ui-polkadot",
  "main": "index.js",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:generate:settings": "npx typedoc --options typedoc.js --tsconfig ./packages/vue-settings/tsconfig.json --out ./docs/vue-settingsx/ ./packages/vue-settings/src/",
    "docs:generate:keyring": "npx typedoc --options typedoc.js --tsconfig ./packages/vue-keyring/tsconfig.json --out ./docs/vue-keyringx/ ./packages/vue-keyring/src/",
    "docs:generate:identicon": "npx typedoc --options typedoc.js --tsconfig ./packages/vue-identicon/tsconfig.json --out ./docs/vue-identiconx/ ./packages/vue-identicon/src/"
  },
  "keywords": [
    "polkadot",
    "vue"
  ],
  "author": "matej nemcek",
  "license": "MIT",
  "devDependencies": {
    "vuepress": "^1.0.3"
  },
  "dependencies": {
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.5",
    "typedoc-plugin-no-inherit": "^1.1.10"
  }
}
